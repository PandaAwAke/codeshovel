         }
 
         while (i < JLS_ORDER.length) {
             if (modifier.getType() == TokenTypes.ANNOTATION) {
                 //Annotation not at start of modifiers, bad
                 return modifier;
             }
 
             while ((i < JLS_ORDER.length)
